<link href="stylesheets/auth.css" rel="stylesheet" type="text/css">
<form class="auth-form" action="/auth/login" accept-charset="UTF-8" method="post">
    <div class="auth-head">
        <p id="title" class="h5">로그인</p>
    </div>
    <div class="auth-detail">
        <div class="form-group">
            <label for="input-id">ID</label>
            <input class="form-control" id="input-id" name="userID" type="text" placeholder="ID" required>
        </div>
        <div class="form-group">
            <label for="input-password">비밀번호</label>
            <input class="form-control" id="input-password" name="userPassword" type="password" placeholder="비밀번호" required>
        </div>
    </div>
    <div class="auth-foot">
        <button type="submit" class="btn btn-primary">로그인</button>
    </div>
</form>
<script src='http://code.jquery.com/jquery-latest.js'></script>
